portable_microhaplotype_object:TravelInfo a sh:NodeShape ;
    sh:closed true ;
    sh:description "Information on travel info" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:double ;
            sh:description "approximate usage of bed net while traveling, 1 = 100% nights with bed net, 0 = 0% no bed net usage" ;
            sh:maxCount 1 ;
            sh:minInclusive 0 ;
            sh:nodeKind sh:Literal ;
            sh:order 7 ;
            sh:path portable_microhaplotype_object:bed_net_usage ],
        [ sh:datatype xsd:string ;
            sh:description "geographical admin level 1, the secondary large demarcation of a nation (nation = admin level 0)" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:geo_admin1 ],
        [ sh:datatype xsd:string ;
            sh:description "the date of the start of travel, can be approximate, should be YYYY-MM or YYYY-MM-DD (preferred)" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 5 ;
            sh:path portable_microhaplotype_object:travel_start_date ;
            sh:pattern "\\d{4}-(?:0[1-9]|1[0-2])(?:-(?:0[1-9]|[12][0-9]|3[01]))?" ],
        [ sh:datatype xsd:string ;
            sh:description "the latitude and longitude of a specific site" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:lat_lon ;
            sh:pattern "^[-+]?\\d{1,2}(?:\\.\\d+)?,[-+]?\\d{1,3}(?:\\.\\d+)?$" ],
        [ sh:datatype xsd:string ;
            sh:description "geographical admin level 2, the third large demarcation of a nation (nation = admin level 0)" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path portable_microhaplotype_object:geo_admin2 ],
        [ sh:datatype xsd:string ;
            sh:description "geographical admin level 3, the third large demarcation of a nation (nation = admin level 0)" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path portable_microhaplotype_object:geo_admin3 ],
        [ sh:datatype xsd:string ;
            sh:description "the name of country, would be the same as admin level 0" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path portable_microhaplotype_object:travel_country ;
            sh:pattern "^[A-Za-z0-9 ,._:'â€“-]+$" ],
        [ sh:datatype xsd:string ;
            sh:description "the date of the end of travel, can be approximate, should be YYYY-MM or YYYY-MM-DD (preferred)" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 6 ;
            sh:path portable_microhaplotype_object:travel_end_date ;
            sh:pattern "\\d{4}-(?:0[1-9]|1[0-2])(?:-(?:0[1-9]|[12][0-9]|3[01]))?" ] ;
    sh:targetClass portable_microhaplotype_object:TravelInfo .

portable_microhaplotype_object:ParasiteDensity a sh:NodeShape ;
    sh:closed true ;

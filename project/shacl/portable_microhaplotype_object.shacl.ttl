@prefix portable_microhaplotype_object: <https://plasmogenepi.github.io/portable-microhaplotype-object/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

portable_microhaplotype_object:MarkerOfInterest a sh:NodeShape ;
    sh:closed true ;
    sh:description "A specific genomic location of interest, e.g. drug resistance, or other phenotypical marker" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "a list of associations with this marker, e.g. SP resistance, etc" ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:associations ;
            sh:pattern "^[A-z-._0-9]+$" ],
        [ sh:class portable_microhaplotype_object:GenomicLocation ;
            sh:description "the genomic location" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:marker_location ] ;
    sh:targetClass portable_microhaplotype_object:MarkerOfInterest .

portable_microhaplotype_object:PortableMicrohaplotypeObject a sh:NodeShape ;
    sh:closed true ;
    sh:description "Information on final results from a targeted amplicon analysis" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class portable_microhaplotype_object:MicrohaplotypesDetected ;
            sh:description "the microhaplotypes detected in this projects" ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 9 ;
            sh:path portable_microhaplotype_object:microhaplotypes_detected ],
        [ sh:class portable_microhaplotype_object:TargetInfo ;
            sh:description "a list of info on the targets" ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 4 ;
            sh:path portable_microhaplotype_object:target_info ],
        [ sh:class portable_microhaplotype_object:PmoHeader ;
            sh:description "the PMO information for this file including version etc" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 10 ;
            sh:path portable_microhaplotype_object:pmo_header ],
        [ sh:class portable_microhaplotype_object:SpecimenInfo ;
            sh:description "a list of specimens of all the samples within this project" ;
            sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:specimen_info ],
        [ sh:class portable_microhaplotype_object:RepresentativeMicrohaplotypes ;
            sh:description "a list of the information on the representative microhaplotypes" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 6 ;
            sh:path portable_microhaplotype_object:microhaplotypes_info ],
        [ sh:class portable_microhaplotype_object:SequencingInfo ;
            sh:description "a list of sequencing info for this project" ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 2 ;
            sh:path portable_microhaplotype_object:sequencing_info ],
        [ sh:class portable_microhaplotype_object:ReadCountsByStage ;
            sh:description "the read counts for different stages of the pipeline" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 11 ;
            sh:path portable_microhaplotype_object:read_counts_by_stage ],
        [ sh:class portable_microhaplotype_object:PanelInfo ;
            sh:description "a list of info on the panels" ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 3 ;
            sh:path portable_microhaplotype_object:panel_info ],
        [ sh:class portable_microhaplotype_object:BioinformaticsRunInfo ;
            sh:description "the runtime info for the bioinformatics pipeline used to generated the amplicon analysis for this project" ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 8 ;
            sh:path portable_microhaplotype_object:bioinformatics_run_info ],
        [ sh:class portable_microhaplotype_object:BioinformaticsMethodInfo ;
            sh:description "the bioinformatics pipeline/methods used to generated the amplicon analysis for this project" ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 7 ;
            sh:path portable_microhaplotype_object:bioinformatics_methods_info ],
        [ sh:class portable_microhaplotype_object:ExperimentInfo ;
            sh:description "a list of experiments of all the seq/amp of the specimens within this project" ;
            sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:experiment_info ],
        [ sh:class portable_microhaplotype_object:GenomeInfo ;
            sh:description "a list of genomes that the targets in TargetInfo refer to" ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 5 ;
            sh:path portable_microhaplotype_object:targeted_genomes ] ;
    sh:targetClass portable_microhaplotype_object:PortableMicrohaplotypeObject .

portable_microhaplotype_object:BioinformaticsMethodInfo a sh:NodeShape ;
    sh:closed true ;
    sh:description "the targeted amplicon bioinformatics pipeline" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "name of the collection of methods is called, e.g. pipeline " ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path portable_microhaplotype_object:bioinformatics_method_name ;
            sh:pattern "^[A-z-._0-9 ]+$" ],
        [ sh:class portable_microhaplotype_object:BioMethod ;
            sh:description "the method used to de-noise and/or cluster the raw reads" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:denoising_method ],
        [ sh:class portable_microhaplotype_object:BioMethod ;
            sh:description "the demultiplexing method used to separate raw reads from barcodes and primer targets" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:demultiplexing_method ],
        [ sh:class portable_microhaplotype_object:BioMethod ;
            sh:description "any additional methods used to analyze the data" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 2 ;
            sh:path portable_microhaplotype_object:additional_methods ] ;
    sh:targetClass portable_microhaplotype_object:BioinformaticsMethodInfo .

portable_microhaplotype_object:BioinformaticsRunInfo a sh:NodeShape ;
    sh:closed true ;
    sh:description "Information about the pipeline run that generated some of the microhaplotype detected and reads_by_stage" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "the date when the run was done, should be YYYY-MM-DD" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:run_date ;
            sh:pattern "\\d{4}-(?:0[1-9]|1[0-2])(?:-(?:0[1-9]|[12][0-9]|3[01]))?" ],
        [ sh:datatype xsd:integer ;
            sh:description "the index into the bioinformatics_methods_info list" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:bioinformatics_methods_id ;
            sh:pattern "^[0-9]+$" ],
        [ sh:datatype xsd:string ;
            sh:description "a name to for this run" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path portable_microhaplotype_object:bioinformatics_run_name ;
            sh:pattern "^[A-z-._0-9 ]+$" ] ;
    sh:targetClass portable_microhaplotype_object:BioinformaticsRunInfo .

portable_microhaplotype_object:ExperimentInfo a sh:NodeShape ;
    sh:closed true ;
    sh:description "Information about a specific amplification and sequencing of a specimen" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "ERA/SRA accession number for the sample if it was submitted" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 6 ;
            sh:path portable_microhaplotype_object:accession ;
            sh:pattern "^[A-z-._0-9]+$" ],
        [ sh:datatype xsd:string ;
            sh:description "a unique identifier for this sequence/amplification run on a specimen_name" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 7 ;
            sh:path portable_microhaplotype_object:experiment_sample_name ;
            sh:pattern "^[A-z-._0-9 ]+$" ],
        [ sh:datatype xsd:integer ;
            sh:description "the index into the panel_info list" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 5 ;
            sh:path portable_microhaplotype_object:panel_id ;
            sh:pattern "^[0-9]+$" ],
        [ sh:datatype xsd:string ;
            sh:description "a name of plate the specimen was in" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:plate_name ;
            sh:pattern "^[A-z-._0-9 ]+$" ],
        [ sh:datatype xsd:integer ;
            sh:description "the column the specimen was in" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path portable_microhaplotype_object:plate_col ;
            sh:pattern "^[0-9]+$" ],
        [ sh:datatype xsd:integer ;
            sh:description "the index into the sequencing_info list" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:sequencing_info_id ;
            sh:pattern "^[0-9]+$" ],
        [ sh:datatype xsd:string ;
            sh:description "the row the specimen was in" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path portable_microhaplotype_object:plate_row ;
            sh:pattern "^[A-z]$" ],
        [ sh:datatype xsd:integer ;
            sh:description "the index into the specimen_info list" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path portable_microhaplotype_object:specimen_id ;
            sh:pattern "^[0-9]+$" ] ;
    sh:targetClass portable_microhaplotype_object:ExperimentInfo .

portable_microhaplotype_object:GenomeInfo a sh:NodeShape ;
    sh:closed true ;
    sh:description "information on a genome" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:integer ;
            sh:description "the NCBI taxonomy number" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path portable_microhaplotype_object:taxon_id ;
            sh:pattern "^[0-9]$" ],
        [ sh:datatype xsd:string ;
            sh:description "name of the genome" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:name ;
            sh:pattern "^[A-z-._0-9]+$" ],
        [ sh:datatype xsd:string ;
            sh:description "a link to the where this genome's annotation file could be downloaded" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 5 ;
            sh:path portable_microhaplotype_object:gff_url ;
            sh:pattern "^(https?|ftp):\\/\\/[^\\s/$.?#].[^\\s]*$" ],
        [ sh:datatype xsd:string ;
            sh:description "a link to the where this genome file could be downloaded" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path portable_microhaplotype_object:url ;
            sh:pattern "^(https?|ftp):\\/\\/[^\\s/$.?#].[^\\s]*$" ],
        [ sh:datatype xsd:string ;
            sh:description "the genome version" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:genome_version ;
            sh:pattern "^[A-z-._0-9]+$" ],
        [ sh:datatype xsd:string ;
            sh:description "a list of chromosomes found within this genome" ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path portable_microhaplotype_object:chromosomes ;
            sh:pattern "^[A-z-._0-9]+$" ] ;
    sh:targetClass portable_microhaplotype_object:GenomeInfo .

portable_microhaplotype_object:MaskingInfo a sh:NodeShape ;
    sh:closed true ;
    sh:description "information about a subsegment of the sequence that should be masked" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:integer ;
            sh:description "the size of the masking" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:seq_segment_size ],
        [ sh:datatype xsd:integer ;
            sh:description "the start of the masking" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:seq_start ],
        [ sh:datatype xsd:integer ;
            sh:description "the size of replacement mask" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path portable_microhaplotype_object:replacement_size ] ;
    sh:targetClass portable_microhaplotype_object:MaskingInfo .

portable_microhaplotype_object:MicrohaplotypeForTarget a sh:NodeShape ;
    sh:closed true ;
    sh:description "Microhaplotype detected for a specific target" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:integer ;
            sh:description "the unique molecular identifier (umi) count associated with this microhaplotype" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path portable_microhaplotype_object:umis ],
        [ sh:datatype xsd:integer ;
            sh:description "the read count associated with this microhaplotype" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:reads ],
        [ sh:datatype xsd:integer ;
            sh:description "the index for a microhaplotype for a target in the microhaplotypes_info list, e.g. microhaplotypes_info[mhaps_target_id][mhap_id]" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:mhap_id ;
            sh:pattern "^[0-9]+$" ] ;
    sh:targetClass portable_microhaplotype_object:MicrohaplotypeForTarget .

portable_microhaplotype_object:MicrohaplotypesDetected a sh:NodeShape ;
    sh:closed true ;
    sh:description "the microhaplotypes detected in a targeted amplicon analysis" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class portable_microhaplotype_object:MicrohaplotypesForSample ;
            sh:description "a list of the microhaplotypes detected for a sample by targets " ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:experiment_samples ],
        [ sh:datatype xsd:integer ;
            sh:description "the index into bioinformatics_run_info list" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:bioinformatics_run_id ;
            sh:pattern "^[0-9]+$" ] ;
    sh:targetClass portable_microhaplotype_object:MicrohaplotypesDetected .

portable_microhaplotype_object:MicrohaplotypesForSample a sh:NodeShape ;
    sh:closed true ;
    sh:description "Microhaplotypes detected for a sample for all targets" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class portable_microhaplotype_object:MicrohaplotypesForTarget ;
            sh:description "a list of the microhaplotypes detected for a list of targets" ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:target_results ],
        [ sh:datatype xsd:integer ;
            sh:description "the index into the experiment_info list" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:experiment_sample_id ;
            sh:pattern "^[0-9]+$" ] ;
    sh:targetClass portable_microhaplotype_object:MicrohaplotypesForSample .

portable_microhaplotype_object:MicrohaplotypesForTarget a sh:NodeShape ;
    sh:closed true ;
    sh:description "Microhaplotypes detected for a specific target" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:integer ;
            sh:description "the index for a target in the microhaplotypes_info list" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:mhaps_target_id ;
            sh:pattern "^[0-9]+$" ],
        [ sh:class portable_microhaplotype_object:MicrohaplotypeForTarget ;
            sh:description "a list of the microhaplotypes detected for this target" ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:haps ] ;
    sh:targetClass portable_microhaplotype_object:MicrohaplotypesForTarget .

portable_microhaplotype_object:PanelInfo a sh:NodeShape ;
    sh:closed true ;
    sh:description "information on a panel of targeted amplicon primer pairs" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "a name for the panel" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:panel_name ;
            sh:pattern "^[A-z-._0-9]+$" ],
        [ sh:class portable_microhaplotype_object:ReactionInfo ;
            sh:description "a list of 1 or more reactions that this panel contains, each reactions list the targets that were amplified in that reaction, e.g. pool1, pool2" ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:reactions ] ;
    sh:targetClass portable_microhaplotype_object:PanelInfo .

portable_microhaplotype_object:ParasiteDensity a sh:NodeShape ;
    sh:closed true ;
    sh:description "method and value of determined parasite density" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:double ;
            sh:description "the density in microliters" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:minInclusive 0 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:density ;
            sh:pattern "^[-+]?\\\\d*\\\\.?\\\\d+(?:[eE][-+]?\\\\d+)?$" ],
        [ sh:datatype xsd:string ;
            sh:description "the method of how this density was obtained" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:method ;
            sh:pattern "^[A-z-._0-9 ]+$" ] ;
    sh:targetClass portable_microhaplotype_object:ParasiteDensity .

portable_microhaplotype_object:PmoGenerationMethod a sh:NodeShape ;
    sh:closed true ;
    sh:description "Information about how a PMO was generated" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "the name of the program" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:program_name ;
            sh:pattern "^[A-z-._0-9 ]+$" ],
        [ sh:datatype xsd:string ;
            sh:description "the version of generation method, should be in the format of v[MAJOR].[MINOR].[PATCH]" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:program_version ;
            sh:pattern "^[A-z-._0-9 ]+$" ] ;
    sh:targetClass portable_microhaplotype_object:PmoGenerationMethod .

portable_microhaplotype_object:PmoHeader a sh:NodeShape ;
    sh:closed true ;
    sh:description "Information on the PMO file" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "the version of the PMO file, should be in the format of v[MAJOR].[MINOR].[PATCH]" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:pmo_version ;
            sh:pattern "^[A-z-._0-9 ]+$" ],
        [ sh:class portable_microhaplotype_object:PmoGenerationMethod ;
            sh:description "the generation method to create this PMO " ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 2 ;
            sh:path portable_microhaplotype_object:generation_method ],
        [ sh:datatype xsd:string ;
            sh:description "the date of when the PMO file was created or modified, should be YYYY-MM-DD" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:creation_date ;
            sh:pattern "\\d{4}-(?:0[1-9]|1[0-2])(?:-(?:0[1-9]|[12][0-9]|3[01]))?" ] ;
    sh:targetClass portable_microhaplotype_object:PmoHeader .

portable_microhaplotype_object:ReactionInfo a sh:NodeShape ;
    sh:closed true ;
    sh:description "information on a panel of targeted amplicon primer pairs" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "a name for this reaction" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:reaction_name ;
            sh:pattern "^[A-z-._0-9]+$" ],
        [ sh:datatype xsd:integer ;
            sh:description "a list of the target indexes in the target_info list" ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:panel_targets ] ;
    sh:targetClass portable_microhaplotype_object:ReactionInfo .

portable_microhaplotype_object:ReadCountsByStage a sh:NodeShape ;
    sh:closed true ;
    sh:description "Information on the reads counts at several stages of a pipeline" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:integer ;
            sh:description "the index into bioinformatics_run_info list" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:bioinformatics_run_id ;
            sh:pattern "^[0-9]+$" ],
        [ sh:class portable_microhaplotype_object:ReadCountsByStageForExperimentalSample ;
            sh:description "a list by experiment_sample for the counts at each stage" ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:read_counts_by_experimental_sample_by_stage ] ;
    sh:targetClass portable_microhaplotype_object:ReadCountsByStage .

portable_microhaplotype_object:ReadCountsByStageForExperimentalSample a sh:NodeShape ;
    sh:closed true ;
    sh:description "Information on the reads counts at several stages of a pipeline for a experimental_sample" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:integer ;
            sh:description "the raw counts off the sequencing machine that a sample began with" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:total_raw_count ;
            sh:pattern "^[0-9]+$" ],
        [ sh:datatype xsd:integer ;
            sh:description "the index into the experiment_info list" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:experiment_sample_id ;
            sh:pattern "^[0-9]+$" ],
        [ sh:class portable_microhaplotype_object:ReadCountsByStageForTarget ;
            sh:description "a list of counts by stage for a target" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 2 ;
            sh:path portable_microhaplotype_object:read_counts_for_targets ] ;
    sh:targetClass portable_microhaplotype_object:ReadCountsByStageForExperimentalSample .

portable_microhaplotype_object:ReadCountsByStageForTarget a sh:NodeShape ;
    sh:closed true ;
    sh:description "Information on the reads counts at several stages of a pipeline for a target" ;

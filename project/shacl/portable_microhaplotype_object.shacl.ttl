portable_microhaplotype_object:GenomicLocation a sh:NodeShape ;
    sh:closed true ;
    sh:description "information on the genomic location of specific sequence" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "a possible alternative sequence of this genomic location" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 6 ;
            sh:path portable_microhaplotype_object:alt_seq ;
            sh:pattern "^[A-z-]+$" ],
        [ sh:datatype xsd:string ;
            sh:description "the reference sequence of this genomic location" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 5 ;
            sh:path portable_microhaplotype_object:ref_seq ;
            sh:pattern "^[A-z-]+$" ],
        [ sh:datatype xsd:integer ;
            sh:description "the end of the location, 0-based positioning" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:minInclusive 0 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path portable_microhaplotype_object:end ;
            sh:pattern "^[0-9]+$" ],
        [ sh:datatype xsd:integer ;
            sh:description "the index to the genome in the targeted_genomes list that this location refers to" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:minInclusive 0 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:genome_id ;
            sh:pattern "^[0-9]+$" ],
        [ sh:datatype xsd:integer ;
            sh:description "the start of the location, 0-based positioning" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:minInclusive 0 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path portable_microhaplotype_object:start ;
            sh:pattern "^[0-9]+$" ],
        [ sh:datatype xsd:string ;
            sh:description "which strand the location is, either + for plus strand or - for negative strand" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path portable_microhaplotype_object:strand ;
            sh:pattern "[+-]" ],
        [ sh:datatype xsd:string ;
            sh:description "the chromosome name" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:chrom ;
            sh:pattern "^[A-z-._0-9]+$" ] ;
    sh:targetClass portable_microhaplotype_object:GenomicLocation .


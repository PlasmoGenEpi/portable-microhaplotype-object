@prefix portable_microhaplotype_object: <https://plasmogenepi.github.io/portable-microhaplotype-object/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

portable_microhaplotype_object:PortableMicrohaplotypeObject a sh:NodeShape ;
    sh:closed true ;
    sh:description "Information on final results from a targeted amplicon analysis" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "a name for the analysis detailed here in this experiment, can be a concatenation of names if combined more than one PMO" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:analysis_name ],
        [ sh:class portable_microhaplotype_object:TarAmpBioinformaticsInfo ;
            sh:description "the bioinformatics pipeline/methods used to generated the amplicon analysis for this project" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 8 ;
            sh:path portable_microhaplotype_object:taramp_bioinformatics_infos ],
        [ sh:class portable_microhaplotype_object:ExperimentInfo ;
            sh:description "a list of experiments of all the seq/amp of samples within this project" ;
            sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:experiment_infos ],
        [ sh:class portable_microhaplotype_object:MicrohaplotypesDetected ;
            sh:description "the microhaplotypes detected in this projects" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 6 ;
            sh:path portable_microhaplotype_object:microhaplotypes_detected ],
        [ sh:class portable_microhaplotype_object:SequencingInfo ;
            sh:description "the sequencing info for this project" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 3 ;
            sh:path portable_microhaplotype_object:sequencing_infos ],
        [ sh:class portable_microhaplotype_object:PanelInfo ;
            sh:description "the info on the panel used for this project" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 4 ;
            sh:path portable_microhaplotype_object:panel_info ],
        [ sh:class portable_microhaplotype_object:DemultiplexedExperimentSamples ;
            sh:description "the raw demultiplex target counts for each sample" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 7 ;
            sh:path portable_microhaplotype_object:target_demultiplexed_experiment_samples ],
        [ sh:class portable_microhaplotype_object:SpecimenInfo ;
            sh:description "a list of SpecimenInfo of all the samples within this project" ;
            sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 2 ;
            sh:path portable_microhaplotype_object:specimen_infos ],
        [ sh:class portable_microhaplotype_object:RepresentativeMicrohaplotypeSequences ;
            sh:description "a list of the representative sequences for the results for this project" ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 5 ;
            sh:path portable_microhaplotype_object:representative_microhaplotype_sequences ],
        [ sh:class portable_microhaplotype_object:BioMethod ;
            sh:description "any additional methods that were applied to the processing of this file/analysis, this can be filtering, adding info etc" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 9 ;
            sh:path portable_microhaplotype_object:postprocessing_bioinformatics_infos ] ;
    sh:targetClass portable_microhaplotype_object:PortableMicrohaplotypeObject .

portable_microhaplotype_object:DemultiplexedExperimentSamples a sh:NodeShape ;
    sh:closed true ;
    sh:description "a list of raw reads counts for each experiment sample for all targets within panel" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "a unique identifier for this targeted amplicon bioinformatics pipeline run" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:tar_amp_bioinformatics_info_id ;
            sh:pattern "^[A-z-._0-9 ]$" ],
        [ sh:class portable_microhaplotype_object:DemultiplexedTargetsForExperimentSample ;
            sh:description "a list of the samples with the number of raw reads extracted " ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:demultiplexed_experiment_samples ] ;
    sh:targetClass portable_microhaplotype_object:DemultiplexedExperimentSamples .

portable_microhaplotype_object:DemultiplexedTargetForExperimentSample a sh:NodeShape ;
    sh:closed true ;
    sh:description "the raw read count for a experiment sample for a target" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:double ;
            sh:description "the raw read counts extracted for a target for a experiment sample" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:raw_read_count ],
        [ sh:datatype xsd:string ;
            sh:description "name of the target" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:target_id ;
            sh:pattern "^[A-z-._0-9]$" ] ;
    sh:targetClass portable_microhaplotype_object:DemultiplexedTargetForExperimentSample .

portable_microhaplotype_object:DemultiplexedTargetsForExperimentSample a sh:NodeShape ;
    sh:closed true ;
    sh:description "a list of raw reads for a experiment sample for all targets within panel" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class portable_microhaplotype_object:DemultiplexedTargetForExperimentSample ;
            sh:description "a list of the targets extracted for a sample " ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:demultiplexed_targets ],
        [ sh:datatype xsd:string ;
            sh:description "a unique identifier for this sequence/amplification run on a specimen" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:experiment_sample_id ;
            sh:pattern "^[A-z-._0-9 ]$" ] ;
    sh:targetClass portable_microhaplotype_object:DemultiplexedTargetsForExperimentSample .

portable_microhaplotype_object:ExperimentInfo a sh:NodeShape ;
    sh:closed true ;
    sh:description "Information about a specific amplification and sequencing of a specimen" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "a unique identifier for this sequencing info" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:sequencing_info_id ;
            sh:pattern "^[A-z-._0-9 ]$" ],
        [ sh:datatype xsd:string ;
            sh:description "a name of plate the specimen was in" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:plate_name ;
            sh:pattern "^[A-z-._0-9 ]$" ],
        [ sh:datatype xsd:string ;
            sh:description "the row the specimen was in" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path portable_microhaplotype_object:plate_row ;
            sh:pattern "^[A-z]$" ],
        [ sh:datatype xsd:string ;
            sh:description "a unique identifier for this sequence/amplification run on a specimen" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 6 ;
            sh:path portable_microhaplotype_object:experiment_sample_id ;
            sh:pattern "^[A-z-._0-9 ]$" ],
        [ sh:datatype xsd:string ;
            sh:description "the name of the specimen of a individual" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path portable_microhaplotype_object:specimen_id ;
            sh:pattern "^[A-z-._0-9]$" ],
        [ sh:datatype xsd:string ;
            sh:description "name of the panel" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 5 ;
            sh:path portable_microhaplotype_object:panel_id ;
            sh:pattern "^[A-z-._0-9]$" ],
        [ sh:datatype xsd:integer ;
            sh:description "the column the specimen was in" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path portable_microhaplotype_object:plate_col ;
            sh:pattern "^[0-9]$" ] ;
    sh:targetClass portable_microhaplotype_object:ExperimentInfo .

portable_microhaplotype_object:GenomeInfo a sh:NodeShape ;
    sh:closed true ;
    sh:description "information on a genome" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "a link to the where this genome file could be downloaded" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path portable_microhaplotype_object:url ;
            sh:pattern "r\"^(https?|ftp):\\/\\/[^\\s/$.?#].[^\\s]*$\"" ],
        [ sh:datatype xsd:string ;
            sh:description "the genome version" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:version ;
            sh:pattern "^[A-z-._0-9]$" ],
        [ sh:datatype xsd:integer ;
            sh:description "the NCBI taxonomy number" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path portable_microhaplotype_object:taxon_id ;
            sh:pattern "^[0-9]$" ],
        [ sh:datatype xsd:string ;
            sh:description "a link to the where this genome's annotation file could be downloaded" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path portable_microhaplotype_object:gff_url ;
            sh:pattern "r\"^(https?|ftp):\\/\\/[^\\s/$.?#].[^\\s]*$\"" ],
        [ sh:datatype xsd:string ;
            sh:description "name of the genome" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:name ;
            sh:pattern "^[A-z-._0-9]$" ] ;
    sh:targetClass portable_microhaplotype_object:GenomeInfo .

portable_microhaplotype_object:MaskingInfo a sh:NodeShape ;
    sh:closed true ;
    sh:description "information about a subsegment of the sequence that should be masked" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:integer ;
            sh:description "the start of the masking" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:seq_start ],
        [ sh:datatype xsd:integer ;
            sh:description "the size of the masking" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:seq_segment_size ],
        [ sh:datatype xsd:integer ;
            sh:description "the size of replacement mask" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path portable_microhaplotype_object:replacement_size ] ;
    sh:targetClass portable_microhaplotype_object:MaskingInfo .

portable_microhaplotype_object:MicrohaplotypeForTarget a sh:NodeShape ;
    sh:closed true ;
    sh:description "Microhaplotype detected for a specific target" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:double ;
            sh:description "the unique molecular identifier (umi) count associated with this microhaplotype" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path portable_microhaplotype_object:umi_count ],
        [ sh:datatype xsd:string ;
            sh:description "name of the microhaplotype, should be unique to this microhaplotype" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:microhaplotype_id ;
            sh:pattern "^[A-z-._0-9]$" ],
        [ sh:datatype xsd:double ;
            sh:description "the read count associated with this microhaplotype" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:read_count ] ;
    sh:targetClass portable_microhaplotype_object:MicrohaplotypeForTarget .

portable_microhaplotype_object:MicrohaplotypesDetected a sh:NodeShape ;
    sh:closed true ;
    sh:description "the microhaplotypes detected in a targeted amplicon analysis" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class portable_microhaplotype_object:MicrohaplotypesForSample ;
            sh:description "a list of the microhaplotypes detected for a sample for various targets " ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:experiment_samples ],
        [ sh:datatype xsd:string ;
            sh:description "a unique identifier for this targeted amplicon bioinformatics pipeline run" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:tar_amp_bioinformatics_info_id ;
            sh:pattern "^[A-z-._0-9 ]$" ] ;
    sh:targetClass portable_microhaplotype_object:MicrohaplotypesDetected .

portable_microhaplotype_object:MicrohaplotypesForSample a sh:NodeShape ;
    sh:closed true ;
    sh:description "Microhaplotypes detected for a sample for all targets" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class portable_microhaplotype_object:MicrohaplotypesForTarget ;
            sh:description "a list of the microhaplotypes detected for a list of targets" ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:target_results ],
        [ sh:datatype xsd:string ;
            sh:description "a unique identifier for this sequence/amplification run on a specimen" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:experiment_sample_id ;
            sh:pattern "^[A-z-._0-9 ]$" ] ;
    sh:targetClass portable_microhaplotype_object:MicrohaplotypesForSample .

portable_microhaplotype_object:MicrohaplotypesForTarget a sh:NodeShape ;
    sh:closed true ;
    sh:description "Microhaplotypes detected for a specific target" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "name of the target" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:target_id ;
            sh:pattern "^[A-z-._0-9]$" ],
        [ sh:class portable_microhaplotype_object:MicrohaplotypeForTarget ;
            sh:description "a list of the microhaplotypes detected for this target" ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:microhaplotypes ] ;
    sh:targetClass portable_microhaplotype_object:MicrohaplotypesForTarget .

portable_microhaplotype_object:PanelInfo a sh:NodeShape ;
    sh:closed true ;
    sh:description "information on a panel of targeted amplicon primer pairs" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "name of the panel" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:panel_id ;
            sh:pattern "^[A-z-._0-9]$" ],
        [ sh:class portable_microhaplotype_object:TargetInfo ;
            sh:description "a list of the target infos for the targets in this panel " ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 2 ;
            sh:path portable_microhaplotype_object:targets ],
        [ sh:class portable_microhaplotype_object:GenomeInfo ;
            sh:description "the info on the target reference genome for this panel" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:target_genome ] ;
    sh:targetClass portable_microhaplotype_object:PanelInfo .

portable_microhaplotype_object:RepresentativeMicrohaplotypeSequence a sh:NodeShape ;
    sh:closed true ;
    sh:description "the representative sequence for a microhaplotype, similar to a fast(a/q) format" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "the pseudocigar of the haplotype" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path portable_microhaplotype_object:pseudocigar ;
            sh:pattern "^[A-z-._0-9]$" ],
        [ sh:datatype xsd:string ;
            sh:description "the DNA sequence" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:seq ;
            sh:pattern "^[A-z]$" ],
        [ sh:datatype xsd:string ;
            sh:description "a list of additional annotations associated with this microhaplotype, e.g. wildtype, amino acid changes etc" ;
            sh:nodeKind sh:Literal ;
            sh:order 5 ;
            sh:path portable_microhaplotype_object:alt_annotations ],
        [ sh:datatype xsd:string ;
            sh:description "name of the microhaplotype, should be unique to this microhaplotype" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:microhaplotype_id ;
            sh:pattern "^[A-z-._0-9]$" ],
        [ sh:class portable_microhaplotype_object:MaskingInfo ;
            sh:description "masking info for the sequence" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 4 ;
            sh:path portable_microhaplotype_object:masking ],
        [ sh:datatype xsd:string ;
            sh:description "the ansi fastq per base quality score for this sequence, this is optional" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path portable_microhaplotype_object:quality ;
            sh:pattern "^[A-z-._0-9]$" ] ;
    sh:targetClass portable_microhaplotype_object:RepresentativeMicrohaplotypeSequence .

portable_microhaplotype_object:RepresentativeMicrohaplotypeSequences a sh:NodeShape ;
    sh:closed true ;
    sh:description "a list of the representative sequence for a microhaplotypes, similar to a fast(a/q) format" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class portable_microhaplotype_object:RepresentativeMicrohaplotypeSequence ;
            sh:description "a list of the microhaplotypes detected for a sample for various targets " ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:seqs ],
        [ sh:datatype xsd:string ;
            sh:description "name of the target" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:target_id ;
            sh:pattern "^[A-z-._0-9]$" ] ;
    sh:targetClass portable_microhaplotype_object:RepresentativeMicrohaplotypeSequences .

portable_microhaplotype_object:SequencingInfo a sh:NodeShape ;
    sh:closed true ;
    sh:description "Information on sequencing info" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "Describe enrichment, screening, or normalization methods applied during amplification or library preparation, e.g. size selection 390bp, diluted to 1 ng DNA/sample" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 8 ;
            sh:path portable_microhaplotype_object:lib_screen ;
            sh:pattern "^[A-z-._0-9 ]$" ],
        [ sh:datatype xsd:string ;
            sh:description "the date of the nucleoacide extraction" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 5 ;
            sh:path portable_microhaplotype_object:nucl_acid_ext_date ;
            sh:pattern "r\"\\d{4}-(?:0[1-9]|1[0-2])(?:-(?:0[1-9]|[12][0-9]|3[01]))?\"" ],
        [ sh:datatype xsd:string ;
            sh:description "a unique identifier for this sequencing info" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:sequencing_info_id ;
            sh:pattern "^[A-z-._0-9 ]$" ],
        [ sh:datatype xsd:string ;
            sh:description "the method/conditions for PCR, List PCR cycles used to amplify the target" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 7 ;
            sh:path portable_microhaplotype_object:pcr_cond ;
            sh:pattern "^[A-z-._0-9 ]$" ],
        [ sh:datatype xsd:string ;
            sh:description "Link to a reference or kit that describes the enzymatic amplification of nucleic acids," ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path portable_microhaplotype_object:nucl_acid_amp ;
            sh:pattern "r\"^(https?|ftp):\\/\\/[^\\s/$.?#].[^\\s]*$\"" ],
        [ sh:datatype xsd:string ;
            sh:description "Name of facility where sequencing was performed (lab, core facility, or company)" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 11 ;
            sh:path portable_microhaplotype_object:seq_center ;
            sh:pattern "^[A-z-._0-9 ]$" ],
        [ sh:datatype xsd:string ;
            sh:description "Specify the configuration of reads, e.g. paired-end" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 9 ;
            sh:path portable_microhaplotype_object:lib_layout ;
            sh:pattern "^[A-z-._0-9 ]$" ],
        [ sh:datatype xsd:string ;
            sh:description "the date of the nucleoacide amplification" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 6 ;
            sh:path portable_microhaplotype_object:nucl_acid_amp_date ;
            sh:pattern "r\"\\d{4}-(?:0[1-9]|1[0-2])(?:-(?:0[1-9]|[12][0-9]|3[01]))?\"" ],
        [ sh:datatype xsd:string ;
            sh:description "the sequencing instrument used to sequence the run, e.g. ILLUMINA, Illumina MiSeq" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:seq_instrument ;
            sh:pattern "^[A-z-._0-9 ]$" ],
        [ sh:datatype xsd:string ;
            sh:description "Name, version, and applicable cell or cycle numbers for the kit used to prepare libraries and load cells or chips for sequencing. If possible, include a part number, e.g. MiSeq Reagent Kit v3 (150-cycle), MS-102-3001" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 10 ;
            sh:path portable_microhaplotype_object:lib_kit ;
            sh:pattern "^[A-z-._0-9 ]$" ],
        [ sh:datatype xsd:string ;
            sh:description "Link to a reference or kit that describes the recovery of nucleic acids from the sample" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path portable_microhaplotype_object:nucl_acid_ext ;
            sh:pattern "r\"^(https?|ftp):\\/\\/[^\\s/$.?#].[^\\s]*$\"" ],
        [ sh:datatype xsd:string ;
            sh:description "the date of sequencing, should be YYYY-MM or YYYY-MM-DD" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path portable_microhaplotype_object:seq_date ;
            sh:pattern "r\"\\d{4}-(?:0[1-9]|1[0-2])(?:-(?:0[1-9]|[12][0-9]|3[01]))?\"" ] ;
    sh:targetClass portable_microhaplotype_object:SequencingInfo .


            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:seq_platform ;
            sh:pattern "^[A-z-._0-9 ]+$" ],

    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "the date the qpcr was performed, can be YYYY, YYYY-MM, or YYYY-MM-DD" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path portable_microhaplotype_object:date_measured ;
            sh:pattern "(?:\\d{4}(?:-(?:0[1-9]|1[0-2])(?:-(?:0[1-9]|[12][0-9]|3[01]))?)?|NA)" ],
        [ sh:datatype xsd:double ;
            sh:description "the density in microliters" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:minInclusive 0 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:parasite_density ;
            sh:pattern "^[-+]?\\\\d*\\\\.?\\\\d+(?:[eE][-+]?\\\\d+)?$" ],
        [ sh:datatype xsd:string ;
            sh:description "additional comments about how the density was performed" ;
            sh:maxCount 1 ;

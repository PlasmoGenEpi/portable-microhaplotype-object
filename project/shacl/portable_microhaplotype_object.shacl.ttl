@prefix portable_microhaplotype_object: <https://plasmogenepi.github.io/portable-microhaplotype-object/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

portable_microhaplotype_object:PortableMicrohaplotypeObject a sh:NodeShape ;
    sh:closed true ;
    sh:description "Information on final microhaplotype results from a targeted amplicon analysis with associated meta data" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class portable_microhaplotype_object:SpecimenInfo ;
            sh:description "a list of all the specimens within this PMO file" ;
            sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:specimen_info ],
        [ sh:class portable_microhaplotype_object:GenomeInfo ;
            sh:description "a list of genomes that any genomic location information refers to" ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 5 ;
            sh:path portable_microhaplotype_object:targeted_genomes ],
        [ sh:class portable_microhaplotype_object:DetectedMicrohaplotypes ;
            sh:description "the microhaplotypes detected in this projects" ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 9 ;
            sh:path portable_microhaplotype_object:detected_microhaplotypes ],
        [ sh:class portable_microhaplotype_object:BioinformaticsMethodInfo ;
            sh:description "the bioinformatics pipeline/methods used to generated the microhaplotype analysis for this project" ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 7 ;
            sh:path portable_microhaplotype_object:bioinformatics_methods_info ],
        [ sh:class portable_microhaplotype_object:LibrarySampleInfo ;
            sh:description "a list of libraries of all the seq/amp of the specimens within this PMO file" ;
            sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:library_sample_info ],
        [ sh:class portable_microhaplotype_object:PanelInfo ;
            sh:description "a list of info on the panels" ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 3 ;
            sh:path portable_microhaplotype_object:panel_info ],
        [ sh:class portable_microhaplotype_object:ReadCountsByStage ;
            sh:description "the read counts for library_samples for different stages of the pipeline" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 12 ;
            sh:path portable_microhaplotype_object:read_counts_by_stage ],
        [ sh:class portable_microhaplotype_object:TargetInfo ;
            sh:description "a list of info on the targets" ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 4 ;
            sh:path portable_microhaplotype_object:target_info ],
        [ sh:class portable_microhaplotype_object:ProjectInfo ;
            sh:description "the information about the projects stored in this PMO" ;
            sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 10 ;
            sh:path portable_microhaplotype_object:project_info ],
        [ sh:class portable_microhaplotype_object:PmoHeader ;
            sh:description "the PMO information for this file including version etc" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 11 ;
            sh:path portable_microhaplotype_object:pmo_header ],
        [ sh:class portable_microhaplotype_object:BioinformaticsRunInfo ;
            sh:description "the runtime info for the bioinformatics pipeline used to generated the microhaplotypes analysis for this project" ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 8 ;
            sh:path portable_microhaplotype_object:bioinformatics_run_info ],
        [ sh:class portable_microhaplotype_object:RepresentativeMicrohaplotypes ;
            sh:description "a list of the information on the representative microhaplotypes" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 6 ;
            sh:path portable_microhaplotype_object:representative_microhaplotypes ],
        [ sh:class portable_microhaplotype_object:SequencingInfo ;
            sh:description "a list of sequencing infos for this PMO file" ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 2 ;
            sh:path portable_microhaplotype_object:sequencing_info ] ;
    sh:targetClass portable_microhaplotype_object:PortableMicrohaplotypeObject .

portable_microhaplotype_object:BioMethod a sh:NodeShape ;
    sh:closed true ;
    sh:description "bioinformatics methodology description with info on program, version, and arguments different from the default" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "any additional arguments that differ from the default arguments" ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path portable_microhaplotype_object:additional_argument ;
            sh:pattern "^[A-z-._0-9{}\\(\\),\\/\\ ]+$" ],
        [ sh:datatype xsd:string ;
            sh:description "a short description of what this method does" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path portable_microhaplotype_object:program_description ;
            sh:pattern "^[A-z-._0-9\\(\\),\\/\\ ]+$" ],
        [ sh:datatype xsd:string ;
            sh:description "the version of program, should be in the format of v[MAJOR].[MINOR].[PATCH]" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:program_version ;
            sh:pattern "^[A-z-._0-9 ]+$" ],
        [ sh:datatype xsd:string ;
            sh:description "a url pointing to code base of a program, e.g. a github link" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path portable_microhaplotype_object:program_url ;
            sh:pattern "^[A-z-._0-9\\(\\),\\/\\ ]+$" ],
        [ sh:datatype xsd:string ;
            sh:description "name of the program used for this portion of the pipeline" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:program ;
            sh:pattern "^[A-z-._0-9 ]+$" ] ;
    sh:targetClass portable_microhaplotype_object:BioMethod .

portable_microhaplotype_object:BioinformaticsMethodInfo a sh:NodeShape ;
    sh:closed true ;
    sh:description "the targeted amplicon bioinformatics methods used to generate the microhaplotype data in this PMO" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class portable_microhaplotype_object:BioMethod ;
            sh:description "methodology used to generate the microhaplotype data stored in this PMO, e.g. demultiplexing method, denosing method, or a pipeline method that ties all th steps together" ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:methods ] ;
    sh:targetClass portable_microhaplotype_object:BioinformaticsMethodInfo .

portable_microhaplotype_object:BioinformaticsRunInfo a sh:NodeShape ;
    sh:closed true ;
    sh:description "Information about the pipeline run that generated microhaplotype_detected and reads_by_stage" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "a name to for this run, needs to be unique to each run " ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path portable_microhaplotype_object:bioinformatics_run_name ;
            sh:pattern "^[A-z-._0-9 ]+$" ],
        [ sh:datatype xsd:string ;
            sh:description "the date when the run was done, should be YYYY-MM-DD" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:run_date ;
            sh:pattern "\\d{4}-(?:0[1-9]|1[0-2])(?:-(?:0[1-9]|[12][0-9]|3[01]))?" ],
        [ sh:datatype xsd:integer ;
            sh:description "the index into the bioinformatics_methods_info list" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:minInclusive 0 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:bioinformatics_methods_id ;
            sh:pattern "^[0-9]+$" ] ;
    sh:targetClass portable_microhaplotype_object:BioinformaticsRunInfo .

portable_microhaplotype_object:DetectedMicrohaplotypes a sh:NodeShape ;
    sh:closed true ;
    sh:description "the microhaplotypes detected in a targeted amplicon analysis" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:integer ;
            sh:description "the index into bioinformatics_run_info list" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:minInclusive 0 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:bioinformatics_run_id ;
            sh:pattern "^[0-9]+$" ],
        [ sh:class portable_microhaplotype_object:DetectedMicrohaplotypesForSample ;
            sh:description "a list of the microhaplotypes detected for all samples with a list for each target " ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:library_samples ] ;
    sh:targetClass portable_microhaplotype_object:DetectedMicrohaplotypes .

portable_microhaplotype_object:DetectedMicrohaplotypesForSample a sh:NodeShape ;
    sh:closed true ;
    sh:description "Microhaplotypes detected for a sample for all targets" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class portable_microhaplotype_object:DetectedMicrohaplotypesForTarget ;
            sh:description "a list of the microhaplotypes detected for a list of targets" ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:target_results ],
        [ sh:datatype xsd:integer ;
            sh:description "the index into the library_sample_info list" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:minInclusive 0 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:library_sample_id ;
            sh:pattern "^[0-9]+$" ] ;
    sh:targetClass portable_microhaplotype_object:DetectedMicrohaplotypesForSample .

portable_microhaplotype_object:DetectedMicrohaplotypesForTarget a sh:NodeShape ;
    sh:closed true ;
    sh:description "Microhaplotypes detected for a specific target" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class portable_microhaplotype_object:MicrohaplotypeForTarget ;
            sh:description "a list of the microhaplotypes detected for this target" ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:mhaps ],
        [ sh:datatype xsd:integer ;
            sh:description "the index for a target in the representative_microhaplotypes list" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:minInclusive 0 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:mhaps_target_id ;
            sh:pattern "^[0-9]+$" ] ;
    sh:targetClass portable_microhaplotype_object:DetectedMicrohaplotypesForTarget .

portable_microhaplotype_object:GenomeInfo a sh:NodeShape ;
    sh:closed true ;
    sh:description "information on a genome" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "a list of the chromosomes/contigs found within this genome" ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path portable_microhaplotype_object:chromosomes ;
            sh:pattern "^[A-z-._0-9]+$" ],
        [ sh:datatype xsd:string ;
            sh:description "a link to the where this genome file could be downloaded" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path portable_microhaplotype_object:url ;
            sh:pattern "^(https?|ftp):\\/\\/[^\\s/$.?#].[^\\s]*$" ],
        [ sh:datatype xsd:string ;
            sh:description "the genome version" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:genome_version ;
            sh:pattern "^[A-z-._0-9]+$" ],
        [ sh:datatype xsd:string ;
            sh:description "name of the genome" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:name ;
            sh:pattern "^[A-z-._0-9]+$" ],
        [ sh:datatype xsd:string ;
            sh:description "a link to the where this genome's annotation file could be downloaded" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 5 ;
            sh:path portable_microhaplotype_object:gff_url ;
            sh:pattern "^(https?|ftp):\\/\\/[^\\s/$.?#].[^\\s]*$" ],
        [ sh:datatype xsd:integer ;
            sh:description "the NCBI taxonomy number, can be a list of values if it's a genome file that has been created by combining gnomes from different species" ;
            sh:minCount 1 ;
            sh:minInclusive 0 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path portable_microhaplotype_object:taxon_id ;
            sh:pattern "^[0-9]$" ] ;
    sh:targetClass portable_microhaplotype_object:GenomeInfo .

portable_microhaplotype_object:LibrarySampleInfo a sh:NodeShape ;
    sh:closed true ;
    sh:description "Information about a specific amplification and sequencing of a specimen" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:integer ;
            sh:description "the index into the specimen_info list" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:minInclusive 0 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:specimen_id ;
            sh:pattern "^[0-9]+$" ],
        [ sh:datatype xsd:integer ;
            sh:description "the index into the panel_info list" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:minInclusive 0 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path portable_microhaplotype_object:panel_id ;
            sh:pattern "^[0-9]+$" ],
        [ sh:datatype xsd:string ;
            sh:description "the location (url or filename path) of the fastqs for a library run" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path portable_microhaplotype_object:fastqs_loc ;
            sh:pattern "^[A-z-.;,_0-9\\(\\),\\/\\ ]+$" ],
        [ sh:class portable_microhaplotype_object:PlateInfo ;
            sh:description "plate location of where library was prepared for sequencing " ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 8 ;
            sh:path portable_microhaplotype_object:library_prep_plate_info ],
        [ sh:datatype xsd:string ;
            sh:description "a unique identifier for this sequencing/amplification run" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 7 ;
            sh:path portable_microhaplotype_object:library_sample_name ;
            sh:pattern "^[A-z-._0-9 ]+$" ],
        [ sh:datatype xsd:string ;
            sh:description "ERA/SRA run accession number for the sample if it was submitted" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 5 ;
            sh:path portable_microhaplotype_object:run_accession ;
            sh:pattern "^[A-z-._0-9]+$" ],
        [ sh:datatype xsd:string ;
            sh:description "a list of alternative names" ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path portable_microhaplotype_object:alternate_identifiers ;
            sh:pattern "^[A-z-._0-9 ]+$" ],
        [ sh:datatype xsd:string ;
            sh:description "ERA/SRA experiment accession number for the sample if it was submitted" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 6 ;
            sh:path portable_microhaplotype_object:experiment_accession ;
            sh:pattern "^[A-z-._0-9]+$" ],
        [ sh:class portable_microhaplotype_object:ParasiteDensity ;
            sh:description "qpcr parasite density measurement for this extracted sample" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 9 ;
            sh:path portable_microhaplotype_object:qpcr_parasite_density_info ],
        [ sh:datatype xsd:integer ;
            sh:description "the index into the sequencing_info list" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:minInclusive 0 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:sequencing_info_id ;
            sh:pattern "^[0-9]+$" ] ;
    sh:targetClass portable_microhaplotype_object:LibrarySampleInfo .

portable_microhaplotype_object:MarkerOfInterest a sh:NodeShape ;
    sh:closed true ;
    sh:description "A specific genomic location of interest, e.g. drug resistance, or other phenotypical marker" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class portable_microhaplotype_object:GenomicLocation ;
            sh:description "the genomic location" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:marker_location ],
        [ sh:datatype xsd:string ;
            sh:description "a list of associations with this marker, e.g. SP resistance, etc" ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:associations ;
            sh:pattern "^[A-z-._0-9]+$" ] ;
    sh:targetClass portable_microhaplotype_object:MarkerOfInterest .

portable_microhaplotype_object:MaskingInfo a sh:NodeShape ;
    sh:closed true ;
    sh:description "information about a subsegment of the sequence that should be masked" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:integer ;
            sh:description "the start of the masking" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:minInclusive 0 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:seq_start ],
        [ sh:datatype xsd:integer ;
            sh:description "the size of replacement mask" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:minInclusive 0 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path portable_microhaplotype_object:replacement_size ],
        [ sh:datatype xsd:integer ;
            sh:description "the size of the masking" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:minInclusive 0 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:seq_segment_size ],
        [ sh:datatype xsd:string ;
            sh:description "a description of how the masking information was generated " ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path portable_microhaplotype_object:masking_generation_description ;
            sh:pattern "^[A-z-._0-9\\(\\),\\/\\ ]+$" ] ;
    sh:targetClass portable_microhaplotype_object:MaskingInfo .

portable_microhaplotype_object:MicrohaplotypeForTarget a sh:NodeShape ;
    sh:closed true ;
    sh:description "Microhaplotype detected for a specific target" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:integer ;
            sh:description "the index for a microhaplotype for a target in the representative_microhaplotypes list, e.g. representative_microhaplotypes[mhaps_target_id][mhap_id]" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:minInclusive 0 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:mhap_id ;
            sh:pattern "^[0-9]+$" ],
        [ sh:datatype xsd:integer ;
            sh:description "the unique molecular identifier (umi) count for this microhaplotype" ;
            sh:maxCount 1 ;
            sh:minInclusive 0 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path portable_microhaplotype_object:umis ],
        [ sh:datatype xsd:integer ;
            sh:description "the read count for this microhaplotype" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:minInclusive 0 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:reads ] ;
    sh:targetClass portable_microhaplotype_object:MicrohaplotypeForTarget .

portable_microhaplotype_object:PanelInfo a sh:NodeShape ;
    sh:closed true ;
    sh:description "information on a panel of targeted amplicon primer pairs" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "a name for the panel" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:panel_name ;
            sh:pattern "^[A-z-._0-9]+$" ],
        [ sh:class portable_microhaplotype_object:ReactionInfo ;
            sh:description "a list of 1 or more reactions that this panel contains, each reactions list the targets that were amplified in that reaction, e.g. pool1, pool2" ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:reactions ] ;
    sh:targetClass portable_microhaplotype_object:PanelInfo .

portable_microhaplotype_object:PmoGenerationMethod a sh:NodeShape ;
    sh:closed true ;
    sh:description "Information about how a PMO was generated" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "the version of program, should be in the format of v[MAJOR].[MINOR].[PATCH]" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:program_version ;
            sh:pattern "^[A-z-._0-9 ]+$" ],
        [ sh:datatype xsd:string ;
            sh:description "the name of the program" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:program_name ;
            sh:pattern "^[A-z-._0-9 ]+$" ] ;
    sh:targetClass portable_microhaplotype_object:PmoGenerationMethod .

portable_microhaplotype_object:PmoHeader a sh:NodeShape ;
    sh:closed true ;
    sh:description "Information on the PMO file itself" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "the date of when the PMO file was created or modified, should be YYYY-MM-DD" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:creation_date ;
            sh:pattern "\\d{4}-(?:0[1-9]|1[0-2])(?:-(?:0[1-9]|[12][0-9]|3[01]))?" ],
        [ sh:datatype xsd:string ;
            sh:description "the version of the PMO file, should be in the format of v[MAJOR].[MINOR].[PATCH]" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:pmo_version ;
            sh:pattern "^[A-z-._0-9 ]+$" ],
        [ sh:class portable_microhaplotype_object:PmoGenerationMethod ;
            sh:description "the generation method to create this PMO " ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 2 ;
            sh:path portable_microhaplotype_object:generation_method ] ;
    sh:targetClass portable_microhaplotype_object:PmoHeader .

portable_microhaplotype_object:ProjectInfo a sh:NodeShape ;
    sh:closed true ;
    sh:description "Information on a project underwhich a collection of specimens belong to" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "a short description of the project" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:project_description ],
        [ sh:datatype xsd:string ;
            sh:description "can be collection of names separated by a semicolon if multiple people involved or can just be the name of the primary person managing the specimen" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path portable_microhaplotype_object:project_collector_chief_scientist ;
            sh:pattern "^[A-z-._0-9;|\\(\\),\\/\\ ]+$" ],
        [ sh:datatype xsd:string ;
            sh:description "a name for the project, should be unique if multiple projects listed" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:project_name ;

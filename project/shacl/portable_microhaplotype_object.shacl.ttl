portable_microhaplotype_object:SequencingInfo a sh:NodeShape ;
    sh:closed true ;
    sh:description "Information on sequencing info" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "a name of for the sequencing done, e.g. batch1" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:sequencing_info_name ;
            sh:pattern "^[A-z-._0-9 ]+$" ],
        [ sh:datatype xsd:string ;
            sh:description "the sequencing instrument model used to sequence the run, e.g. NextSeq 2000, MinION, Revio" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path portable_microhaplotype_object:seq_instrument_model ;
            sh:pattern "^[A-z-._0-9 ]+$" ],
        [ sh:datatype xsd:string ;
            sh:description "Name of facility where sequencing was performed (lab, core facility, or company)" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 15 ;
            sh:path portable_microhaplotype_object:seq_center ;
            sh:pattern "^[A-z-._0-9\\(\\),\\/\\ ]+$" ],
        [ sh:datatype xsd:string ;
            sh:description "the date of sequencing, should be YYYY-MM or YYYY-MM-DD" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path portable_microhaplotype_object:seq_date ;
            sh:pattern "\\d{4}-(?:0[1-9]|1[0-2])(?:-(?:0[1-9]|[12][0-9]|3[01]))?" ],
        [ sh:datatype xsd:string ;
            sh:description "Link to a reference or kit that describes the recovery of nucleic acids from the sample" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path portable_microhaplotype_object:nucl_acid_ext ;
            sh:pattern "^(https?|ftp):\\/\\/[^\\s/$.?#].[^\\s]*$" ],
        [ sh:datatype xsd:string ;
            sh:description "the date of the nucleoacid extraction" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 6 ;
            sh:path portable_microhaplotype_object:nucl_acid_ext_date ;
            sh:pattern "\\d{4}-(?:0[1-9]|1[0-2])(?:-(?:0[1-9]|[12][0-9]|3[01]))?" ],
        [ sh:datatype xsd:string ;
            sh:description "the sequencing technology used to sequence the run, e.g. ILLUMINA, NANOPORE, PACBIO" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:seq_platform ;
            sh:pattern "^[A-z-._0-9 ]+$" ],
        [ sh:datatype xsd:string ;
            sh:description "the method/conditions for PCR, List PCR cycles used to amplify the target" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 8 ;
            sh:path portable_microhaplotype_object:pcr_cond ;
            sh:pattern "^[A-z-._0-9\\(\\),\\/\\ ]+$" ],
        [ sh:datatype xsd:string ;
            sh:description "how amplification was done (common are PCR=Source material was selected by designed primers, RANDOM =Random selection by shearing or other method)" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 14 ;
            sh:path portable_microhaplotype_object:library_selection ;
            sh:pattern "^[A-z-._0-9 ]+$" ],
        [ sh:datatype xsd:string ;
            sh:description "the date of the nucleoacid amplification" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 7 ;
            sh:path portable_microhaplotype_object:nucl_acid_amp_date ;
            sh:pattern "\\d{4}-(?:0[1-9]|1[0-2])(?:-(?:0[1-9]|[12][0-9]|3[01]))?" ],
        [ sh:datatype xsd:string ;
            sh:description "Link to a reference or kit that describes the enzymatic amplification of nucleic acids" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 5 ;
            sh:path portable_microhaplotype_object:nucl_acid_amp ;
            sh:pattern "^(https?|ftp):\\/\\/[^\\s/$.?#].[^\\s]*$" ],
        [ sh:datatype xsd:string ;
            sh:description "Source of amplification material (common names GENOMIC, TRANSCRIPTOMIC)" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 13 ;
            sh:path portable_microhaplotype_object:library_source ;
            sh:pattern "^[A-z-._0-9 ]+$" ],
        [ sh:datatype xsd:string ;
            sh:description "Specify the configuration of reads, e.g. paired-end, single" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 11 ;
            sh:path portable_microhaplotype_object:library_layout ;
            sh:pattern "^[A-z-._0-9 ]+$" ],
        [ sh:datatype xsd:string ;
            sh:description "Describe enrichment, screening, or normalization methods applied during amplification or library preparation, e.g. size selection 390bp, diluted to 1 ng DNA/sample" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 9 ;
            sh:path portable_microhaplotype_object:library_screen ;
            sh:pattern "^[A-z-._0-9\\(\\),\\/\\ ]+$" ],
        [ sh:datatype xsd:string ;
            sh:description "Name, version, and applicable cell or cycle numbers for the kit used to prepare libraries and load cells or chips for sequencing. If possible, include a part number, e.g. MiSeq Reagent Kit v3 (150-cycle), MS-102-3001" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 10 ;
            sh:path portable_microhaplotype_object:library_kit ;
            sh:pattern "^[A-z-._0-9\\(\\),\\/\\ ]+$" ],
        [ sh:datatype xsd:string ;
            sh:description "what the nuceloacid sequencing/amplification strategy was (common names are AMPLICON, WGS)" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 12 ;
            sh:path portable_microhaplotype_object:library_strategy ;
            sh:pattern "^[A-z-._0-9 ]+$" ] ;
    sh:targetClass portable_microhaplotype_object:SequencingInfo .


@prefix portable_microhaplotype_object: <https://plasmogenepi.github.io/portable-microhaplotype-object/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

portable_microhaplotype_object:PortableMicrohaplotypeObject a sh:NodeShape ;
    sh:closed true ;
    sh:description "Information on final results from a targeted amplicon analysis" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class portable_microhaplotype_object:SequencingInfo ;
            sh:description "the sequencing info for this project" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:sequencing_info ],
        [ sh:class portable_microhaplotype_object:TarAmpBioinformaticsInfo ;
            sh:description "the bioinformatics pipeline info for this project" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 2 ;
            sh:path portable_microhaplotype_object:bioinformatics_info ],
        [ sh:class portable_microhaplotype_object:HaplotypesDetected ;
            sh:description "the haplotypes detected in this projects" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 5 ;
            sh:path portable_microhaplotype_object:haplotypes_detected ],
        [ sh:class portable_microhaplotype_object:SpecimenInfo ;
            sh:description "a list of SpecimenInfo of all the samples within this project" ;
            sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:specimen_infos ],
        [ sh:class portable_microhaplotype_object:RepresentativeHaplotypeSequences ;
            sh:description "a list of the representative sequences for the results for this project" ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 4 ;
            sh:path portable_microhaplotype_object:representative_haplotype_sequences ],
        [ sh:class portable_microhaplotype_object:DemultiplexedSamples ;
            sh:description "the raw demultiplex target counts for each sample  " ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 6 ;
            sh:path portable_microhaplotype_object:target_demultiplexed_samples ],
        [ sh:class portable_microhaplotype_object:PanelInfo ;
            sh:description "the info on the panel used for this project" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 3 ;
            sh:path portable_microhaplotype_object:panel_info ] ;
    sh:targetClass portable_microhaplotype_object:PortableMicrohaplotypeObject .

portable_microhaplotype_object:DemultiplexedSamples a sh:NodeShape ;
    sh:closed true ;
    sh:description "a list of raw reads counts for each sample for all targets within panel " ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class portable_microhaplotype_object:DemultiplexedTargetsForSample ;
            sh:description "a list of the samples with the number of raw reads extracted " ;
            sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:demultiplexed_samples ] ;
    sh:targetClass portable_microhaplotype_object:DemultiplexedSamples .

portable_microhaplotype_object:DemultiplexedTargetForSample a sh:NodeShape ;
    sh:closed true ;
    sh:description "the raw read count for a sample for a target" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:double ;
            sh:description "the raw read counts extracted for a target for a sample" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:raw_read_count ],
        [ sh:datatype xsd:string ;
            sh:description "name of the target" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:target_id ;
            sh:pattern "^[A-z-._0-9]$" ] ;
    sh:targetClass portable_microhaplotype_object:DemultiplexedTargetForSample .

portable_microhaplotype_object:DemultiplexedTargetsForSample a sh:NodeShape ;
    sh:closed true ;
    sh:description "a list of raw reads for a sample for all targets within panel" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class portable_microhaplotype_object:DemultiplexedTargetForSample ;
            sh:description "a list of the targets extracted for a sample " ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:demultiplexed_targets ],
        [ sh:datatype xsd:string ;
            sh:description "the name of the sample" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:sample_id ;
            sh:pattern "^[A-z-._0-9]$" ] ;
    sh:targetClass portable_microhaplotype_object:DemultiplexedTargetsForSample .

portable_microhaplotype_object:GenomeInfo a sh:NodeShape ;
    sh:closed true ;
    sh:description "information on a genome" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:integer ;
            sh:description "the NCBI taxonomy number" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path portable_microhaplotype_object:taxon_id ;
            sh:pattern "^[0-9]$" ],
        [ sh:datatype xsd:string ;
            sh:description "the genome version" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:version ;
            sh:pattern "^[A-z-._0-9]$" ],
        [ sh:datatype xsd:string ;
            sh:description "a link to the where this genome's annotation file could be downloaded" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path portable_microhaplotype_object:gff_url ;
            sh:pattern "r\"^(https?|ftp):\\/\\/[^\\s/$.?#].[^\\s]*$\"" ],
        [ sh:datatype xsd:string ;
            sh:description "name of the genome" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:name ;
            sh:pattern "^[A-z-._0-9]$" ],
        [ sh:datatype xsd:string ;
            sh:description "a link to the where this genome file could be downloaded" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path portable_microhaplotype_object:url ;
            sh:pattern "r\"^(https?|ftp):\\/\\/[^\\s/$.?#].[^\\s]*$\"" ] ;
    sh:targetClass portable_microhaplotype_object:GenomeInfo .

portable_microhaplotype_object:HaplotypeForTarget a sh:NodeShape ;
    sh:closed true ;
    sh:description "Haplotype detected for a specific target" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:double ;
            sh:description "the read count associated with this haplotype" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:read_count ],
        [ sh:datatype xsd:string ;
            sh:description "name of the haplotype, should be unique to this haplotype" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:haplotype_id ;
            sh:pattern "^[A-z-._0-9]$" ],
        [ sh:datatype xsd:double ;
            sh:description "the unique molecular identifier (umi) count associated with this haplotype" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path portable_microhaplotype_object:umi_count ] ;
    sh:targetClass portable_microhaplotype_object:HaplotypeForTarget .

portable_microhaplotype_object:HaplotypesDetected a sh:NodeShape ;
    sh:closed true ;
    sh:description "the haplotypes detected in a targeted amplicon analysis" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "the name of the sequencing/wet lab processing steps associated with this run" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:sequencing_id ;
            sh:pattern "^[A-z-._0-9]$" ],
        [ sh:class portable_microhaplotype_object:HaplotypesForSample ;
            sh:description "a list of the haplotypes detected for a sample for various targets " ;
            sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 2 ;
            sh:path portable_microhaplotype_object:samples ],
        [ sh:datatype xsd:string ;
            sh:description "the name of the bioinformatics processing steps associated with this run" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:bioinformatics_id ;
            sh:pattern "^[A-z-._0-9]$" ] ;
    sh:targetClass portable_microhaplotype_object:HaplotypesDetected .

portable_microhaplotype_object:HaplotypesForSample a sh:NodeShape ;
    sh:closed true ;
    sh:description "Haplotypes detected for a sample for all targets" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class portable_microhaplotype_object:HaplotypesForTarget ;
            sh:description "a list of the haplotypes detected for a list of targets" ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:target_results ],
        [ sh:datatype xsd:string ;
            sh:description "the name of the sample" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:sample_id ;
            sh:pattern "^[A-z-._0-9]$" ] ;
    sh:targetClass portable_microhaplotype_object:HaplotypesForSample .

portable_microhaplotype_object:HaplotypesForTarget a sh:NodeShape ;
    sh:closed true ;
    sh:description "Haplotypes detected for a specific target" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class portable_microhaplotype_object:HaplotypeForTarget ;
            sh:description "a list of the haplotypes detected for this target" ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:haplotypes ],
        [ sh:datatype xsd:string ;
            sh:description "name of the target" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:target_id ;
            sh:pattern "^[A-z-._0-9]$" ] ;
    sh:targetClass portable_microhaplotype_object:HaplotypesForTarget .

portable_microhaplotype_object:PanelInfo a sh:NodeShape ;
    sh:closed true ;
    sh:description "information on a panel of targeted amplicon primer pairs" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class portable_microhaplotype_object:GenomeInfo ;
            sh:description "the info on the target reference genome for this panel" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:target_genome ],
        [ sh:class portable_microhaplotype_object:TargetInfo ;
            sh:description "a list of the target infos for the targets in this panel " ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 2 ;
            sh:path portable_microhaplotype_object:targets ],
        [ sh:datatype xsd:string ;
            sh:description "name of the panel" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:panel_id ;
            sh:pattern "^[A-z-._0-9]$" ] ;
    sh:targetClass portable_microhaplotype_object:PanelInfo .

portable_microhaplotype_object:PrimerInfo a sh:NodeShape ;
    sh:closed true ;
    sh:description "information on a primer sequence" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "the DNA sequence" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:seq ;
            sh:pattern "^[A-z]$" ],
        [ sh:class portable_microhaplotype_object:GenomicLocation ;
            sh:description """what the intended genomic location of the primer is  
  """ ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:location ] ;
    sh:targetClass portable_microhaplotype_object:PrimerInfo .

portable_microhaplotype_object:RepresentativeHaplotypeSequence a sh:NodeShape ;
    sh:closed true ;
    sh:description "the representative sequence for a haplotype, similar to a fast(a/q) format" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "the DNA sequence" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:seq ;
            sh:pattern "^[A-z]$" ],
        [ sh:datatype xsd:string ;
            sh:description "the ansi fastq per base quality score for this sequence, this is optional" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path portable_microhaplotype_object:quality ;
            sh:pattern "^[A-z-._0-9]$" ],
        [ sh:datatype xsd:string ;
            sh:description "a list of additional annotations associated with this haplotype, e.g. wildtype, amino acid changes etc" ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path portable_microhaplotype_object:alt_annotations ],
        [ sh:datatype xsd:string ;
            sh:description "name of the haplotype, should be unique to this haplotype" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:haplotype_id ;
            sh:pattern "^[A-z-._0-9]$" ] ;
    sh:targetClass portable_microhaplotype_object:RepresentativeHaplotypeSequence .

portable_microhaplotype_object:RepresentativeHaplotypeSequences a sh:NodeShape ;
    sh:closed true ;
    sh:description "a list of the representative sequence for a haplotypes, similar to a fast(a/q) format" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class portable_microhaplotype_object:RepresentativeHaplotypeSequence ;
            sh:description "a list of the haplotypes detected for a sample for various targets " ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:seqs ],
        [ sh:datatype xsd:string ;
            sh:description "name of the target" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:target_id ;
            sh:pattern "^[A-z-._0-9]$" ] ;
    sh:targetClass portable_microhaplotype_object:RepresentativeHaplotypeSequences .

portable_microhaplotype_object:SequencingInfo a sh:NodeShape ;
    sh:closed true ;
    sh:description "Information on sequencing info" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "Link to a reference or kit that describes the recovery of nucleic acids from the sample" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path portable_microhaplotype_object:nucl_acid_ext ;
            sh:pattern "r\"^(https?|ftp):\\/\\/[^\\s/$.?#].[^\\s]*$\"" ],
        [ sh:datatype xsd:string ;
            sh:description "Name, version, and applicable cell or cycle numbers for the kit used to prepare libraries and load cells or chips for sequencing. If possible, include a part number, e.g. MiSeq Reagent Kit v3 (150-cycle), MS-102-3001" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 9 ;
            sh:path portable_microhaplotype_object:lib_kit ;
            sh:pattern "^[A-z-._0-9 ]$" ],
        [ sh:datatype xsd:string ;
            sh:description "the method/conditions for PCR, List PCR cycles used to amplify the target" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 6 ;
            sh:path portable_microhaplotype_object:pcr_cond ;
            sh:pattern "^[A-z-._0-9 ]$" ],
        [ sh:datatype xsd:string ;
            sh:description "the date of the extraction/amplification" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 5 ;
            sh:path portable_microhaplotype_object:nucl_acid_date ;
            sh:pattern "r\"\\d{4}-(?:0[1-9]|1[0-2])(?:-(?:0[1-9]|[12][0-9]|3[01]))?\"" ],
        [ sh:datatype xsd:string ;
            sh:description "Name of facility where sequencing was performed (lab, core facility, or company)" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 10 ;
            sh:path portable_microhaplotype_object:seq_center ;
            sh:pattern "^[A-z-._0-9 ]$" ],
        [ sh:datatype xsd:string ;
            sh:description "the sequencing instrument used to sequence the run, e.g. ILLUMINA, Illumina MiSeq" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:seq_instrument ;
            sh:pattern "^[A-z-._0-9 ]$" ],
        [ sh:datatype xsd:string ;
            sh:description "Describe enrichment, screening, or normalization methods applied during amplification or library preparation, e.g. size selection 390bp, diluted to 1 ng DNA/sample" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 7 ;
            sh:path portable_microhaplotype_object:lib_screen ;
            sh:pattern "^[A-z-._0-9 ]$" ],
        [ sh:datatype xsd:string ;
            sh:description "Specify the configuration of reads, e.g. paired-end" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 8 ;
            sh:path portable_microhaplotype_object:lib_layout ;
            sh:pattern "^[A-z-._0-9 ]$" ],
        [ sh:datatype xsd:string ;
            sh:description "Link to a reference or kit that describes the enzymatic amplification of nucleic acids," ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path portable_microhaplotype_object:nucl_acid_amp ;
            sh:pattern "r\"^(https?|ftp):\\/\\/[^\\s/$.?#].[^\\s]*$\"" ],
        [ sh:datatype xsd:string ;
            sh:description "the date of sequencing, should be YYYY-MM or YYYY-MM-DD" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path portable_microhaplotype_object:seq_date ;
            sh:pattern "r\"\\d{4}-(?:0[1-9]|1[0-2])(?:-(?:0[1-9]|[12][0-9]|3[01]))?\"" ],
        [ sh:datatype xsd:string ;
            sh:description "a unique identifier for this sequencing info" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:sequencing_info_id ;
            sh:pattern "^[A-z-._0-9 ]$" ] ;
    sh:targetClass portable_microhaplotype_object:SequencingInfo .

portable_microhaplotype_object:SpecimenInfo a sh:NodeShape ;
    sh:closed true ;
    sh:description "Information on specimen info" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "the sample store site, address or facility name" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 8 ;
            sh:path portable_microhaplotype_object:samp_store_loc ;
            sh:pattern "^[A-z-._0-9 ]$" ],
        [ sh:datatype xsd:string ;
            sh:description "ERA/SRA accession number for the sample if it was submitted" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 11 ;
            sh:path portable_microhaplotype_object:accession ;
            sh:pattern "^[A-z-._0-9]$" ],
        [ sh:datatype xsd:integer ;
            sh:description "optional the NCBI taxonomy number of the host of the organism" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path portable_microhaplotype_object:host_taxon_id ;
            sh:pattern "^[0-9]$" ],
        [ sh:datatype xsd:string ;
            sh:description "the name of the primary person managing the specimen" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 7 ;
            sh:path portable_microhaplotype_object:collector ;
            sh:pattern "^[A-z-._0-9 ]$" ],
        [ sh:datatype xsd:string ;
            sh:description "a name of the project under which the sample is organized" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 10 ;
            sh:path portable_microhaplotype_object:project_name ;
            sh:pattern "^[A-z-._0-9 ]$" ],
        [ sh:datatype xsd:integer ;
            sh:description "the parasite density in microliters" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path portable_microhaplotype_object:parasite_density ;
            sh:pattern "r'^[-+]?\\d*\\.?\\d+(?:[eE][-+]?\\d+)?$'" ],
        [ sh:datatype xsd:string ;
            sh:description "the name of the sample" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:sample_id ;
            sh:pattern "^[A-z-._0-9]$" ],
        [ sh:datatype xsd:string ;
            sh:description "the row the specimen was in" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 14 ;
            sh:path portable_microhaplotype_object:plate_row ;
            sh:pattern "^[A-z-._0-9 ]$" ],
        [ sh:datatype xsd:string ;
            sh:description "a list of optional alternative names for the samples" ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path portable_microhaplotype_object:alternate_identifiers ;
            sh:pattern "^[A-z-._0-9 ]$" ],
        [ sh:datatype xsd:string ;
            sh:description "any additional comments about the sample" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 12 ;
            sh:path portable_microhaplotype_object:sample_comments ;
            sh:pattern "^[A-z-._0-9 ]$" ],
        [ sh:datatype xsd:string ;
            sh:description "a name of plate the specimen was in" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 13 ;
            sh:path portable_microhaplotype_object:plate_name ;
            sh:pattern "^[A-z-._0-9 ]$" ],
        [ sh:datatype xsd:string ;
            sh:description "the latitude and longitude of the collection site of the specimen" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 6 ;
            sh:path portable_microhaplotype_object:lat_lon ;
            sh:pattern "r'^[-+]?\\d{1,2}(?:\\.\\d+)?,[-+]?\\d{1,3}(?:\\.\\d+)?$'" ],
        [ sh:datatype xsd:integer ;
            sh:description "the NCBI taxonomy number of the organism of interest" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:samp_taxon_id ;
            sh:pattern "^[0-9]$" ],
        [ sh:datatype xsd:string ;
            sh:description "the date of the sample collection" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 5 ;
            sh:path portable_microhaplotype_object:collection_date ;
            sh:pattern "r\"\\d{4}-(?:0[1-9]|1[0-2])(?:-(?:0[1-9]|[12][0-9]|3[01]))?\"" ],
        [ sh:datatype xsd:string ;
            sh:description "the column the specimen was in " ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;

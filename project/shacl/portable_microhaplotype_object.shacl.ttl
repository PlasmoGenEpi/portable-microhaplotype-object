            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 11 ;
            sh:path portable_microhaplotype_object:library_layout ;
            sh:pattern "^[A-z-._0-9 ]+$" ],
        [ sh:datatype xsd:string ;

portable_microhaplotype_object:RepresentativeMicrohaplotype a sh:NodeShape ;
    sh:closed true ;
    sh:description "the representative sequence for a microhaplotype, similar to a fast(a/q) format" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "the DNA sequence" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path portable_microhaplotype_object:seq ;
            sh:pattern "^[A-z]+$" ],
        [ sh:datatype xsd:string ;
            sh:description "an optional name for this microhaplotype" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path portable_microhaplotype_object:microhaplotype_name ;
            sh:pattern "^[A-z-._0-9]+$" ],
        [ sh:datatype xsd:string ;
            sh:description "a list of additional annotations associated with this microhaplotype, e.g. wildtype, amino acid changes etc" ;
            sh:nodeKind sh:Literal ;
            sh:order 5 ;
            sh:path portable_microhaplotype_object:alt_annotations ],
        [ sh:datatype xsd:string ;
            sh:description "the pseudocigar of the haplotype" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path portable_microhaplotype_object:pseudocigar ;

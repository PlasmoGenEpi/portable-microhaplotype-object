      "slot_uri": "https://plasmogenepi.github.io/portable-microhaplotype-object/specimen_id",
      "owner": "SpecimenInfo",
      "domain_of": [
        "ExperimentInfo",
        "SpecimenInfo"
      ],
      "range": "string",
      "required": true,
      "pattern": "^[A-z-._0-9]$",

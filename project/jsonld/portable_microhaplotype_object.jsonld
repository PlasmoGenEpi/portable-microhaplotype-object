      "from_schema": "https://plasmogenepi.github.io/portable-microhaplotype-object",
      "slot_uri": "https://plasmogenepi.github.io/portable-microhaplotype-object/representative_microhaplotype_id",
      "owner": "MicrohaplotypesDetected",
      "domain_of": [
        "RepresentativeMicrohaplotypeSequences",
        "MicrohaplotypesDetected"
      ],
      "range": "string",
      "required": true,
      "pattern": "^[A-z-._0-9]$",
      "@type": "SlotDefinition"
    },
    {
      "name": "microhaplotype_id",
      "definition_uri": "https://plasmogenepi.github.io/portable-microhaplotype-object/microhaplotype_id",
      "description": "name of the microhaplotype, should be unique to this microhaplotype",
      "from_schema": "https://plasmogenepi.github.io/portable-microhaplotype-object",
      "slot_uri": "https://plasmogenepi.github.io/portable-microhaplotype-object/microhaplotype_id",
      "owner": "MicrohaplotypeForTarget",
      "domain_of": [
        "RepresentativeMicrohaplotypeSequence",

portable_microhaplotype_object:BioMethod a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "BioMethod" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom [ a rdfs:Datatype ;
                    owl:intersectionOf ( linkml:String [ a rdfs:Datatype ;
                                owl:onDatatype xsd:string ;
                                owl:withRestrictions ( [ xsd:pattern "^[A-z-._0-9 ]$" ] ) ] ) ] ;
            owl:onProperty portable_microhaplotype_object:additional_argument ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty portable_microhaplotype_object:program ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty portable_microhaplotype_object:purpose ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a rdfs:Datatype ;
                    owl:intersectionOf ( linkml:String [ a rdfs:Datatype ;
                                owl:onDatatype xsd:string ;
                                owl:withRestrictions ( [ xsd:pattern "^[A-z-._0-9 ]$" ] ) ] ) ] ;
            owl:onProperty portable_microhaplotype_object:program ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty portable_microhaplotype_object:additional_argument ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty portable_microhaplotype_object:program_version ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a rdfs:Datatype ;
                    owl:intersectionOf ( linkml:String [ a rdfs:Datatype ;
                                owl:onDatatype xsd:string ;
                                owl:withRestrictions ( [ xsd:pattern "^[A-z-._0-9 ]$" ] ) ] ) ] ;
            owl:onProperty portable_microhaplotype_object:program_version ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty portable_microhaplotype_object:program_version ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty portable_microhaplotype_object:purpose ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a rdfs:Datatype ;
                    owl:intersectionOf ( linkml:String [ a rdfs:Datatype ;
                                owl:onDatatype xsd:string ;
                                owl:withRestrictions ( [ xsd:pattern "^[A-z-._0-9 ]$" ] ) ] ) ] ;
            owl:onProperty portable_microhaplotype_object:purpose ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty portable_microhaplotype_object:program ] ;
    skos:definition "methodology description of a portion of a bioinformatics pipeline" ;
    skos:inScheme <https://plasmogenepi.github.io/portable-microhaplotype-object> .

portable_microhaplotype_object:haplotype_id a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "haplotype_id" ;
    rdfs:range [ a rdfs:Datatype ;
            owl:intersectionOf ( linkml:String [ a rdfs:Datatype ;

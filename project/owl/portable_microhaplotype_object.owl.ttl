        [ a owl:Restriction ;
            owl:allValuesFrom portable_microhaplotype_object:BioMethod ;
            owl:onProperty portable_microhaplotype_object:demultiplexing_method ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a rdfs:Datatype ;
                    owl:intersectionOf ( linkml:String [ a rdfs:Datatype ;
                                owl:onDatatype xsd:string ;
                                owl:withRestrictions ( [ xsd:pattern "^[A-z-._0-9 ]+$" ] ) ] ) ] ;
            owl:onProperty portable_microhaplotype_object:bioinformatics_method_name ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty portable_microhaplotype_object:bioinformatics_method_name ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty portable_microhaplotype_object:bioinformatics_method_name ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;

portable_microhaplotype_object:PortableMicrohaplotypeObject a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "PortableMicrohaplotypeObject" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom portable_microhaplotype_object:HaplotypesDetected ;
            owl:onProperty portable_microhaplotype_object:haplotypes_detected ],
        [ a owl:Restriction ;
            owl:allValuesFrom portable_microhaplotype_object:RepresentativeHaplotypeSequences ;
            owl:onProperty portable_microhaplotype_object:representative_haplotype_sequences ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty portable_microhaplotype_object:representative_haplotype_sequences ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty portable_microhaplotype_object:bioinformatics_info ],
        [ a owl:Restriction ;
            owl:allValuesFrom portable_microhaplotype_object:DemultiplexedSamples ;
            owl:onProperty portable_microhaplotype_object:target_demultiplexed_samples ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty portable_microhaplotype_object:bioinformatics_info ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty portable_microhaplotype_object:haplotypes_detected ],
        [ a owl:Restriction ;
            owl:allValuesFrom portable_microhaplotype_object:SequencingInfo ;
            owl:onProperty portable_microhaplotype_object:sequencing_info ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty portable_microhaplotype_object:panel_info ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty portable_microhaplotype_object:sequencing_info ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty portable_microhaplotype_object:target_demultiplexed_samples ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty portable_microhaplotype_object:specimen_infos ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty portable_microhaplotype_object:haplotypes_detected ],
        [ a owl:Restriction ;
            owl:allValuesFrom portable_microhaplotype_object:TarAmpBioinformaticsInfo ;
            owl:onProperty portable_microhaplotype_object:bioinformatics_info ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty portable_microhaplotype_object:panel_info ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty portable_microhaplotype_object:sequencing_info ],
        [ a owl:Restriction ;
            owl:allValuesFrom portable_microhaplotype_object:SpecimenInfo ;
            owl:onProperty portable_microhaplotype_object:specimen_infos ],
        [ a owl:Restriction ;
            owl:allValuesFrom portable_microhaplotype_object:PanelInfo ;
            owl:onProperty portable_microhaplotype_object:panel_info ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty portable_microhaplotype_object:target_demultiplexed_samples ] ;
    skos:definition "Information on final results from a targeted amplicon analysis" ;
    skos:inScheme <https://plasmogenepi.github.io/portable-microhaplotype-object> .

portable_microhaplotype_object:DemultiplexedSamples a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "DemultiplexedSamples" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty portable_microhaplotype_object:demultiplexed_samples ],
        [ a owl:Restriction ;
            owl:allValuesFrom portable_microhaplotype_object:DemultiplexedTargetsForSample ;
            owl:onProperty portable_microhaplotype_object:demultiplexed_samples ] ;
    skos:definition "a list of raw reads counts for each sample for all targets within panel " ;
    skos:inScheme <https://plasmogenepi.github.io/portable-microhaplotype-object> .

portable_microhaplotype_object:DemultiplexedTargetForSample a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "DemultiplexedTargetForSample" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty portable_microhaplotype_object:target_id ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty portable_microhaplotype_object:raw_read_count ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty portable_microhaplotype_object:raw_read_count ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty portable_microhaplotype_object:target_id ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:Double ;
            owl:onProperty portable_microhaplotype_object:raw_read_count ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a rdfs:Datatype ;
                    owl:intersectionOf ( linkml:String [ a rdfs:Datatype ;
                                owl:onDatatype xsd:string ;
                                owl:withRestrictions ( [ xsd:pattern "^[A-z-._0-9]$" ] ) ] ) ] ;
            owl:onProperty portable_microhaplotype_object:target_id ] ;
    skos:definition "the raw read count for a sample for a target" ;
    skos:inScheme <https://plasmogenepi.github.io/portable-microhaplotype-object> .

portable_microhaplotype_object:DemultiplexedTargetsForSample a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "DemultiplexedTargetsForSample" ;
    rdfs:subClassOf [ a owl:Restriction ;

portable_microhaplotype_object:GenomicLocation a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "GenomicLocation" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty portable_microhaplotype_object:genome_id ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty portable_microhaplotype_object:end ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a rdfs:Datatype ;
                    owl:intersectionOf ( linkml:Integer [ a rdfs:Datatype ;
                                owl:onDatatype xsd:string ;
                                owl:withRestrictions ( [ xsd:pattern "^[0-9]+$" ] ) ] ) ] ;
            owl:onProperty portable_microhaplotype_object:end ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty portable_microhaplotype_object:start ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty portable_microhaplotype_object:alt_seq ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a rdfs:Datatype ;
                    owl:intersectionOf ( linkml:Integer [ a rdfs:Datatype ;
                                owl:onDatatype xsd:string ;
                                owl:withRestrictions ( [ xsd:pattern "^[0-9]+$" ] ) ] ) ] ;
            owl:onProperty portable_microhaplotype_object:genome_id ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty portable_microhaplotype_object:end ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty portable_microhaplotype_object:strand ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty portable_microhaplotype_object:chrom ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty portable_microhaplotype_object:start ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a rdfs:Datatype ;
                    owl:intersectionOf ( linkml:String [ a rdfs:Datatype ;
                                owl:onDatatype xsd:string ;
                                owl:withRestrictions ( [ xsd:pattern "^[A-z-]+$" ] ) ] ) ] ;
            owl:onProperty portable_microhaplotype_object:ref_seq ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a rdfs:Datatype ;
                    owl:intersectionOf ( linkml:String [ a rdfs:Datatype ;
                                owl:onDatatype xsd:string ;
                                owl:withRestrictions ( [ xsd:pattern "^[A-z-._0-9]+$" ] ) ] ) ] ;
            owl:onProperty portable_microhaplotype_object:chrom ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty portable_microhaplotype_object:genome_id ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty portable_microhaplotype_object:chrom ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty portable_microhaplotype_object:ref_seq ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a rdfs:Datatype ;
                    owl:intersectionOf ( linkml:String [ a rdfs:Datatype ;
                                owl:onDatatype xsd:string ;
                                owl:withRestrictions ( [ xsd:pattern "[+-]" ] ) ] ) ] ;
            owl:onProperty portable_microhaplotype_object:strand ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a rdfs:Datatype ;
                    owl:intersectionOf ( linkml:String [ a rdfs:Datatype ;
                                owl:onDatatype xsd:string ;
                                owl:withRestrictions ( [ xsd:pattern "^[A-z-]+$" ] ) ] ) ] ;
            owl:onProperty portable_microhaplotype_object:alt_seq ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty portable_microhaplotype_object:strand ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty portable_microhaplotype_object:ref_seq ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty portable_microhaplotype_object:alt_seq ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a rdfs:Datatype ;
                    owl:intersectionOf ( linkml:Integer [ a rdfs:Datatype ;
                                owl:onDatatype xsd:string ;
                                owl:withRestrictions ( [ xsd:pattern "^[0-9]+$" ] ) ] ) ] ;
            owl:onProperty portable_microhaplotype_object:start ] ;
    skos:definition "information on the genomic location of specific sequence" ;
    skos:inScheme <https://plasmogenepi.github.io/portable-microhaplotype-object> .


            owl:onProperty portable_microhaplotype_object:nucl_acid_ext ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty portable_microhaplotype_object:sequencing_info_id ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a rdfs:Datatype ;

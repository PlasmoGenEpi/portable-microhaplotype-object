            owl:onProperty portable_microhaplotype_object:demultiplexed_targets ] ;
    skos:definition "a list of raw reads for a sample for all targets within panel" ;
    skos:inScheme <https://plasmogenepi.github.io/portable-microhaplotype-object> .

portable_microhaplotype_object:GenomeInfo a owl:Class,

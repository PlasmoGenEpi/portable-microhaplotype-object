                                owl:withRestrictions ( [ xsd:pattern "^[0-9]$" ] ) ] ) ] ;
            owl:onProperty portable_microhaplotype_object:taxon_id ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;

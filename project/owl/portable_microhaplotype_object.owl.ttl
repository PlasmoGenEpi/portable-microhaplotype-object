portable_microhaplotype_object:MarkerOfInterest a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "MarkerOfInterest" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty portable_microhaplotype_object:marker_location ],
        [ a owl:Restriction ;
            owl:allValuesFrom portable_microhaplotype_object:GenomicLocation ;
            owl:onProperty portable_microhaplotype_object:marker_location ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty portable_microhaplotype_object:marker_location ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a rdfs:Datatype ;
                    owl:intersectionOf ( linkml:String [ a rdfs:Datatype ;
                                owl:onDatatype xsd:string ;
                                owl:withRestrictions ( [ xsd:pattern "^[A-z-._0-9]+$" ] ) ] ) ] ;
            owl:onProperty portable_microhaplotype_object:associations ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty portable_microhaplotype_object:associations ] ;
    skos:definition "A specific genomic location of interest, e.g. drug resistance, or other phenotypical marker" ;
    skos:inScheme <https://plasmogenepi.github.io/portable-microhaplotype-object> .

